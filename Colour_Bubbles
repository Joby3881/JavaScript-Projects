    <script>
        function r(){
            return Math.floor(Math.random()*255)
        }
        function g(){
            return Math.floor(Math.random()*255)
        }
        function b(){
            return Math.floor(Math.random()*255)
        }
       
        let l = document.querySelectorAll(".color");
       
        for(let i of l){

           i.onmouseover = function(){

             i.style.backgroundColor=`rgb(${r()},${g()},${b()})`;
           }
        }
        </script>
 
